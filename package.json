{
  "name": "wized-filter-and-pagination",
  "version": "1.0.0",
  "description": "Wized filter and pagination functionality",
  "source": "src/index.html",
  "type": "module",
  "scripts": {
    "start": "parcel src/index.html",
    "build": "parcel build src/index.html --dist-dir dist --no-source-maps",
    "build:dev": "parcel build src/index.html --dist-dir dist --no-optimize",
    "clean": "rm -rf dist .parcel-cache",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{js,json}\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write",
      "jest --findRelatedTests --bail"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "keywords": [
    "wized",
    "filter",
    "pagination",
    "webflow"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.26.4",
    "@babel/core": "^7.26.9",
    "@babel/preset-env": "^7.26.9",
    "@eslint/js": "^8.56.0",
    "@parcel/compressor-brotli": "^2.13.3",
    "@parcel/compressor-gzip": "^2.13.3",
    "@parcel/optimizer-css": "^2.13.3",
    "@parcel/optimizer-htmlnano": "^2.13.3",
    "@parcel/optimizer-terser": "^2.13.3",
    "@parcel/packager-html": "^2.13.3",
    "@parcel/transformer-css": "^2.13.3",
    "@parcel/transformer-html": "^2.13.3",
    "@parcel/transformer-js": "^2.13.3",
    "@types/jest": "^29.5.12",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.0",
    "parcel": "^2.13.3",
    "prettier": "^3.5.1"
  }
}
